					<row><cell>	38.36		</cell><cell>	-4.105	</cell></row>
					<row><cell>	32.748	</cell><cell>	-3.045	</cell></row>
					<row><cell>	26.664	</cell><cell>	-1.957	</cell></row>
					<row><cell>	21.141	</cell><cell>	-1.146	</cell></row>
					<row><cell>	17.445	</cell><cell>	-0.946	</cell></row>
