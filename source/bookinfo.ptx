        \draw (#3,#1) -- (\fpeval{#3+0.2},#1);
        \draw (#4,#2) -- (\fpeval{#4-0.2},#2); 
        \draw (#4,#1) -- (#4 ,#2); 
        \draw (#4,#1) -- (\fpeval{#4-0.2},#1);
    }
